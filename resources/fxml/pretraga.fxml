<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="300.0" prefWidth="428.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <HBox prefHeight="28.0" prefWidth="428.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="searchField" prefHeight="25.0" prefWidth="263.0" />
            <Button mnemonicParsing="false" onAction="#searchAction" text="Search" />
            <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="56.0" text="Ok" />
            <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="58.0" text="Cancel" />
         </children>
      </HBox>
   </top>
   <center>
      <ScrollPane hbarPolicy="NEVER" pannable="true" prefHeight="200.0" prefWidth="175.0" BorderPane.alignment="CENTER">
         <content>
            <GridPane fx:id="kutijaSlika" alignment="CENTER" focusTraversable="true" hgap="10.0" prefHeight="131.0" prefWidth="131.0" vgap="10.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="128.0" prefWidth="128.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="128.0" prefHeight="128.0" vgrow="SOMETIMES" />
              </rowConstraints>
            </GridPane>
         </content>
      </ScrollPane>
   </center>
</BorderPane>
